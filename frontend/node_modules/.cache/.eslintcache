\"data-table\");\r\n\r\n    // Menambahkan tema \"plain\"\r\n\r\n    doc.autoTable({ html: table, theme: \"grid\" });\r\n    doc.save(\"PPICweek4(user).pdf\");\r\n  };\r\n\r\n  //-----\r\n\r\n  // GET NEW MODEL\r\n\r\n  useEffect(() => {\r\n    fetch(\"http://192.168.101.236:3001/api/get/newmodel\")\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        setDatanewmodel(data);\r\n      })\r\n      .catch((error) => {\r\n        // Tangani error jika permintaan gagal\r\n        console.error(\"Error fetching data:\", error);\r\n      });\r\n  }, []);\r\n\r\n  // ----\r\n\r\n  // GET DATA BERDASARKAN DATE\r\n\r\n  const handleDateChange = (e) => {\r\n    const inputDateTime = e.target.value;\r\n    const formattedDateTime = inputDateTime.replace(\r\n      /(\\d{2})\\/(\\d{2})\\/(\\d{4})\\s(\\d{2}):(\\d{2})/,\r\n      \"$3-$2-$1 $4:$5:00\"\r\n    );\r\n    setDate(formattedDateTime);\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetch(\"http://192.168.101.236:3001/api/get/Week4\")\r\n      .then((response) => response.json())\r\n      .then((json) => {\r\n        // mengubah properti timestamp menjadi tanggal dan Date\r\n        json.forEach((item) => {\r\n          const date = new Date(item.Date);\r\n          const day = date.getDate();\r\n          const month = date.getMonth() + 1;\r\n          const year = date.getFullYear();\r\n          const hours = date.getHours();\r\n          const minutes = date.getMinutes();\r\n          const formattedDate = `${day.toString().padStart(2, \"0\")}-${month\r\n            .toString()\r\n            .padStart(2, \"0\")}-${year} / ${hours\r\n            .toString()\r\n            .padStart(2, \"0\")}:${minutes.toString().padStart(2, \"0\")}`;\r\n          item.Date = formattedDate;\r\n        });\r\n        json.sort((a, b) => Date.parse(a.Date) - Date.parse(b.Date));\r\n        json.reverse();\r\n        setData(json);\r\n        setFilteredData(json);\r\n      });\r\n  }, []);\r\n  //----------\r\n\r\n  // Submit PPIC / ADD\r\n  const SubmitPPIC = (event) => {\r\n    if (!Model || !Pnc || !Week || !date || !Stock) {\r\n      alert(\"Harap isi semua kolom!\");\r\n      return;\r\n    }\r\n\r\n    const data = {\r\n      Model: Model,\r\n      Pnc: Pnc,\r\n      Week: Week,\r\n      Stock: Stock,\r\n      Wip: Wip,\r\n      date: date,\r\n      SE: SE,\r\n      QC: QC,\r\n    };\r\n\r\n    fetch(`http://192.168.101.236:3001/api/post/Week4`, {\r\n      method: \"POST\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n      body: JSON.stringify(data),\r\n    })\r\n      .then((response) => {\r\n        if (response.status === 200) {\r\n          alert(\"Data Berhasil Di Input\");\r\n          setIsModelW2(false);\r\n          window.location.reload();\r\n          event.preventDefault();\r\n        } else {\r\n          throw new Error(\"Error adding data\");\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (Stock !== \"\" && Week !== \"\") {\r\n      const SEValue = parseInt(Stock) - parseInt(Week);\r\n      setSE(SEValue.toString());\r\n    } else {\r\n      setSE(\"\");\r\n    }\r\n  }, [Week, Stock]);\r\n  //----------\r\n\r\n  // BUTTON DELET\r\n  const handleDelete = () => {\r\n    if (selectedItem) {\r\n      // Kirim permintaan ke backend untuk menghapus data dengan ID selectedItem\r\n      fetch(`http://192.168.101.236:3001/api/delete4/${selectedItem}`, {\r\n        method: \"DELETE\",\r\n      })\r\n        .then((response) => response.json())\r\n        .then((result) => {\r\n          alert(\"Data Berhasil Di Hapus\");\r\n          window.location.reload();\r\n          setSelectedItem(null);\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error deleting data:\", error);\r\n        });\r\n    }\r\n  };\r\n  //----------\r\n\r\n  // UPDATE WIP PPIC\r\n  const UpdatePPIC = (event) => {\r\n    const data = {\r\n      Wip: parseInt(Wip),\r\n    };\r\n    fetch(\r\n      `http://192.168.101.236:3001/api/put/Week4PPIC/${selectedItemWip.No}`,\r\n      {\r\n        method: \"PUT\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify(data),\r\n      }\r\n    )\r\n      .then((response) => {\r\n        if (response.status === 200) {\r\n          alert(\"Wip Berhasil Di Tambahkan\");\r\n          window.location.reload();\r\n          event.preventDefault();\r\n        } else {\r\n          throw new Error(\"Error updating data\");\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (selectedItem) {\r\n      setinitialWip(selectedItemWip.Wip);\r\n      setWip(selectedItemWip.Wip);\r\n    }\r\n  }, [selectedItemWip]);\r\n\r\n  useEffect(() => {\r\n    // Mengupdate nilai Wip dengan menambahkan nilai Wip\r\n    const newWip = initialWip + parseInt(Wip);\r\n    setWip(newWip);\r\n  }, [initialWip, Wip]);\r\n  //----------\r\n\r\n  // NEW MODEL / PNC\r\n  const SubmitNew = (event) => {\r\n    const data = {\r\n      NewModel: NewModel,\r\n      NewPnc: NewPnc,\r\n    };\r\n\r\n    fetch(`http://192.168.101.236:3001/api/post/newmodel`, {\r\n      method: \"POST\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n      body: JSON.stringify(data),\r\n    })\r\n      .then((response) => {\r\n        if (response.status === 200) {\r\n          alert(\"Data Berhasil Di Input\");\r\n          setIsModelW2(false);\r\n          window.location.reload();\r\n          event.preventDefault();\r\n        } else {\r\n          throw new Error(\"Error adding data\");\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n  const uniqueModels = Array.from(\r\n    new Set(datanewmodel.map((item) => item.Model))\r\n  );\r\n  //--------\r\n\r\n  return (\r\n 