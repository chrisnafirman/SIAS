setStock] = useState(\"\");\r\n  const [SE, setSE] = useState(\"\");\r\n  const [date, setDate] = useState(\"\");\r\n  const [Wip, setWip] = useState(\"\");\r\n  const [QC, setQC] = useState(\"\");\r\n  const [NewModel, setNewModel] = useState(\"\");\r\n  const [NewPnc, setNewPnc] = useState(\"\");\r\n  const [data, setData] = useState([]);\r\n  const [filteredData, setFilteredData] = useState([]);\r\n  const [datanewmodel, setDatanewmodel] = useState([]);\r\n  const [selectedItem, setSelectedItem] = useState(null);\r\n  const [selectedItemWip, setSelectedItemWip] = useState(null);\r\n  const [initialWip, setinitialWip] = useState(0);\r\n\r\n  // PDF\r\n  const exportToPDF = () => {\r\n    const doc = new jsPDF();\r\n    const table = document.getElementById(\"data-table\");\r\n\r\n    // Menambahkan tema \"plain\"\r\n\r\n    doc.autoTable({ html: table, theme: \"grid\" });\r\n    doc.save(\"PPICweek4.pdf\");\r\n  };\r\n\r\n  //-----\r\n\r\n  // GET NEW MODEL\r\n\r\n  useEffect(() => {\r\n    fetch(\"http://10.14.81.43:3001/api/get/newmodel\")\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        setDatanewmodel(data);\r\n      })\r\n      .catch((error) => {\r\n        // Tangani error jika permintaan gagal\r\n        console.error(\"Error fetching data:\", error);\r\n      });\r\n  }, []);\r\n\r\n  // ----\r\n\r\n  // GET DATA BERDASARKAN DATE\r\n\r\n  const handleDateChange = (e) => {\r\n    const inputDateTime = e.target.value;\r\n    const formattedDateTime = inputDateTime.replace(\r\n      /(\\d{2})\\/(\\d{2})\\/(\\d{4})\\s(\\d{2}):(\\d{2})/,\r\n      \"$3-$2-$1 $4:$5:00\"\r\n    );\r\n    setDate(formattedDateTime);\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetch(\"http://10.14.81.43:3001/api/get/Week4\")\r\n      .then((response) => response.json())\r\n      .then((json) => {\r\n        // mengubah properti timestamp menjadi tanggal dan Date\r\n        json.forEach((item) => {\r\n          const date = new Date(item.Date);\r\n  