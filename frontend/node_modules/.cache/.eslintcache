\"jspdf\";\r\nimport autoTable from \"jspdf-autotable\";\r\n\r\nconst PPICProd = () => {\r\n  const [Week, setWeek] = useState(\"\");\r\n  const [Stock, setStock] = useState(\"\");\r\n  const [SE, setSE] = useState(\"\");\r\n  const [Wip, setWip] = useState(\"\");\r\n  const [QC, setQC] = useState(\"\");\r\n  const [data, setData] = useState([]);\r\n  const [filteredData, setFilteredData] = useState([]);\r\n\r\n  const [selectedItem, setSelectedItem] = useState(null);\r\n\r\n  const [initialStock, setInitialStock] = useState(0);\r\n  const [initialWip, setinitialWip] = useState(0);\r\n  const [initialSE, setinitialSE] = useState(0);\r\n  const [initialWeek, setInitialWeek] = useState(0);\r\n\r\n  const exportToPDF = () => {\r\n    const doc = new jsPDF();\r\n    const table = document.getElementById(\"data-table\");\r\n\r\n    // Menambahkan tema \"plain\"\r\n\r\n    doc.autoTable({ html: table, theme: \"grid\" });\r\n    doc.save(\"QCweek.pdf\");\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetch(\"http://10.14.81.43:3001/api/get/Week4\")\r\n      .then((response) => response.json())\r\n      .then((json) => {\r\n        // mengubah properti timestamp menjadi tanggal dan Date\r\n        json.forEach((item) => {\r\n          const date = new Date(item.Date);\r\n          const day = date.getDate();\r\n          const month = date.getMonth() + 1;\r\n          const year = date.getFullYear();\r\n          const hours = date.getHours();\r\n          const minutes = date.getMinutes();\r\n          const formattedDate = `${day.toString().padStart(2, \"0\")}-${month\r\n            .toString()\r\n            .padStart(2, \"0\")}-${year} / ${hours\r\n            .toString()\r\n            .padStart(2, \"0\")}:${minutes.toString().padStart(2, \"0\")}`;\r\n          item.Date = formattedDate;\r\n        });\r\n        json.sort((a, b) => Date.parse(a.Date) - Date.parse(b.Date));\r\n        json.reverse();\r\n        setData(json);\r\n        setFilteredData(json);\r\n      });\r\n  }, []);\r\n\r\n  // Submit Network\r\n\r\n  useEffect(() => {\r\n    if (Stock !== \"\" && Week !== \"\") {\r\n      const SEValue = parseInt(Stock) - parseInt(Week);\r\n      setSE(SEValue.toString());\r\n    } else {\r\n      setSE(\"\");\r\n    }\r\n  }, [Week, Stock]);\r\n\r\n  //-----------------------------\r\n  useEffect(() => {\r\n    if (selectedItem) {\r\n      setInitialStock(selectedItem.Stock);\r\n      setStock(selectedItem.Stock);\r\n      setInitialWeek(selectedItem.Week);\r\n      setWeek(selectedItem.Week);\r\n      setinitialWip(selectedItem.Wip);\r\n      setWip(selectedItem.Wip);\r\n    }\r\n  }, [selectedItem]);\r\n\r\n  useEffect(() => {\r\n    if (QC === \"\") {\r\n      // Mengatur nilai Stock kembali ke nilai awal\r\n      setWip(initialWip);\r\n      setStock(initialStock);\r\n    } else {\r\n      // Mengupdate nilai Stock dengan menambahkan nilai QC\r\n      const newWip = initialWip - parseInt(QC);\r\n      setWip(newWip);\r\n      const newStock = initialStock + parseInt(QC);\r\n      setStock(newStock);\r\n    }\r\n  }, [QC, initialStock, initialWip]);\r\n\r\n  //\r\n\r\n  useEffect(() => {\r\n    if (Stock !== \"\" && Week !== \"\") {\r\n      const newSE = parseInt(Stock) - parseInt(Week);\r\n      setSE(newSE);\r\n    } else {\r\n      setSE(initialSE);\r\n    }\r\n  }, [Stock, Week, initialSE]);\r\n\r\n  // ----------------------\r\n\r\n  const UpdateQC = (event) => {\r\n    const data = {\r\n      Stock: parseInt(Stock),\r\n      Wip: parseInt(Wip),\r\n      SE: parseInt(SE),\r\n      QC: parseInt(QC),\r\n    };\r\n    fetch(`http://10.14.81.43:3001/api/put/Week4QC/${selectedItem.No}`, {\r\n      method: \"PUT\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n      body: JSON.stringify(data),\r\n    })\r\n      .then((response) => {\r\n        if (response.status === 200) {\r\n          alert(\"Data Telah Berhasil Di Update\");\r\n          window.location.reload();\r\n          event.preventDefault();\r\n        } else {\r\n          throw new Error(\"Error updating data\");\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  return (\r\n    <body\r\n      className=\"h-full w-full\"\r\n      style={{\r\n        height: \"200vh\",\r\n        background: \"linear-gradient(90deg, #697B93 50%, #4A5568 50%)\",\r\n      }}\r\n    >\r\n      <button\r\n        data-drawer-target=\"logo-sidebar\"\r\n        data-drawer-toggle=\"logo-sidebar\"\r\n        aria-controls=\"logo-sidebar\"\r\n        // onClick={toggleDrawer}\r\n        type=\"button\"\r\n        class=\"inline-flex items-center p-2 mt-2 ml-3 text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-600\"\r\n      >\r\n        <span class=\"sr-only\">Open sidebar</span>\r\n        <svg\r\n          class=\"w-6 h-6\"\r\n          aria-hidden=\"true\"\r\n          fill=\"currentColor\"\r\n          viewBox=\"0 0 20 20\"\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\n        >\r\n          <path\r\n            clip-rule=\"evenodd\"\r\n            fill-rule=\"evenodd\"\r\n            d=\"M2 4.75A.75.75 0 012.75 4h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 4.75zm0 10.5a.75.75 0 01.75-.75h7.5a.75.75 0 010 1.5h-7.5a.75.75 0 01-.75-.75zM2 10a.75.75 0 01.75-.75h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 10z\"\r\n          ></path>\r\n        </svg>\r\n      </button>\r\n\r\n      <aside\r\n        id=\"logo-sidebar\"\r\n        class=\"fixed top-0 left-0 z-40 w-44 h-screen transition-transform -translate-x-full  sm:translate-x-0\"\r\n        aria-label=\"Sidebar\"\r\n      >\r\n        <div class=\"h-full px-3 py-4 overflow-y-auto bg-gray-50 dark:bg-gray-800\">\r\n             <img\r\n              src={process.env.PUBLIC_URL + \"/AVI.png\"}\r\n              class=\"h-6 mr-3 sm:h-7\"\r\n              alt=\"Flowbite Logo\"\r\n   