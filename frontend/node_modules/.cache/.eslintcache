;\r\n  //----------\r\n\r\n  // BUTTON DELET\r\n  const handleDelete = () => {\r\n    if (selectedItem) {\r\n      // Kirim permintaan ke backend untuk menghapus data dengan ID selectedItem\r\n      fetch(`http://10.14.81.43:3001/api/delete4/${selectedItem}`, {\r\n        method: \"DELETE\",\r\n      })\r\n        .then((response) => response.json())\r\n        .then((result) => {\r\n          alert(\"Data Berhasil Di Hapus\");\r\n          window.location.reload();\r\n          setSelectedItem(null);\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error deleting data:\", error);\r\n        });\r\n    }\r\n  };\r\n  //----------\r\n\r\n  // UPDATE WIP PPIC\r\n  const UpdatePPIC = (event) => {\r\n    const data = {\r\n      Wip: parseInt(Wip),\r\n    };\r\n    fetch(\r\n      `http://10.14.81.43:3001/api/put/Week4PPIC/${selectedItemWip.No}`,\r\n      {\r\n        method: \"PUT\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify(data),\r\n      }\r\n    )\r\n      .then((response) => {\r\n        if (response.status === 200) {\r\n          alert(\"Wip Berhasil Di Tambahkan\");\r\n          window.location.reload();\r\n          event.preventDefault();\r\n        } else {\r\n          throw new Error(\"Error updating data\");\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (selectedItem) {\r\n      setinitialWip(selectedItemWip.Wip);\r\n      setWip(selectedItemWip.Wip);\r\n    }\r\n  }, [selectedItemWip]);\r\n\r\n  useEffect(() => {\r\n    // Mengupdate nilai Wip dengan menambahkan nilai Wip\r\n    const newWip = initialWip + parseInt(Wip);\r\n    setWip(newWip);\r\n  }, [initialWip, Wip]);\r\n  //----------\r\n\r\n  // NEW MODEL / PNC\r\n  const SubmitNew = (event) => {\r\n    const data = {\r\n      NewModel: NewModel,\r\n      NewPnc: NewPnc,\r\n    };\r\n\r\n    fetch(`http://10.14.81.43:3001/api/post/newmodel`, {\r\n      method: \"POST\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n      body: JSON.stringify(data),\r\n    })\r\n      .then((response) => {\r\n        if (response.status === 200) {\r\n          alert(\"Data Berhasil Di Input\");\r\n          setIsModelW2(false);\r\n          window.location.reload();\r\n          event.preventDefault();\r\n        } else {\r\n          throw new Error(\"Error adding data\");\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n  const uniqueModels = Array.from(\r\n    new Set(datanewmodel.map((item) => item.Model))\r\n  );\r\n  //--------\r\n\r\n  return (\r\n    <body\r\n      className=\"h-full w-full\"\r\n      style={{\r\n        height: \"200vh\",\r\n        background: \"linear-gradient(90deg, #005A5B 50%, #007369 50%)\",\r\n      }}\r\n    >\r\n      <button\r\n        data-drawer-target=\"logo-sidebar\"\r\n        data-drawer-toggle=\"logo-sidebar\"\r\n        aria-controls=\"logo-sidebar\"\r\n        // onClick={toggleDrawer}\r\n        type=\"button\"\r\n        class=\"inline-flex items-center p-2 mt-2 ml-3 text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-600\"\r\n      >\r\n        <span class=\"sr-only\">Open sidebar</span>\r\n        <svg\r\n          class=\"w-6 h-6\"\r\n          aria-hidden=\"true\"\r\n          fill=\"currentColor\"\r\n          viewBox=\"0 0 20 20\"\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\n        >\r\n          <path\r\n            clip-rule=\"evenodd\"\r\n            fill-rule=\"evenodd\"\r\n            d=\"M2 4.75A.75.75 0 012.75 4h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 4.75zm0 10.5a.75.75 0 01.75-.75h7.5a.75.75 0 010 1.5h-7.5a.75.75 0 01-.75-.75zM2 10a.75.75 0 01.75-.75h14.5a.75.75 0 010 1.5H2.75A.75.75 0 012 10z\"\r\n  